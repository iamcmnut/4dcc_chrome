{
  "manifest_version": 2,

  "name": "4D Currency Converter",
  "description": "Real-time currency converter built for Chrome OS and Chrome",
  "version": "0.1.3",
  "app": {
    "background": {
        "scripts": [ "main.js" ]
    }
  },
  "icons": {
      "128": "img/logo-128.png",
      "16": "img/logo-16.png",
      "48": "img/logo-48.png"
   },
  "permissions": [ "http://*.finance.yahoo.com/", "http://*.appspot.com/", "storage" ]
}